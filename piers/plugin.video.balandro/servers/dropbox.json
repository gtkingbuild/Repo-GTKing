{
  "id": "dropbox",
  "name": "Dropbox",

  "active": true,

  "find_videos": {
    "ignore_urls": [],
    "patterns": [
      {
        "pattern": "https://(?:dl|www).(?:dropboxusercontent|dropbox).com/s/([A-z0-9%20]+)/([A-z0-9-.%20]+.(?:mp4|mkv))\\?",
        "url": "https://dl.dropboxusercontent.com/s/\\1/\\2"
      }
    ]
  }

}